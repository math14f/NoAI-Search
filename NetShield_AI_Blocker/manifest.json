{
  "manifest_version": 3,
  "name": "NoAI Search - Skole Edition",
  "version": "1.0",
  "description": "Fjerner kun AI-oversigter og AI-tilstand fra Google SÃ¸gning for at sikre undervisningen. Ingen anden blokering.",
  
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "content_scripts": [
    {
      "matches": [
        "*://*.google.com/search*",
        "*://*.google.dk/search*",
        "*://*.google.se/search*",
        "*://*.google.no/search*",
        "*://*.google.de/search*",
        "*://*.google.co.uk/search*",
        "*://*.google.es/search*",
        "*://*.google.fr/search*",
        "*://*.google.it/search*",
        "*://*.google.nl/search*"
      ],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}